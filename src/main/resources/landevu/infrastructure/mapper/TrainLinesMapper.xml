<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="landevu.infrastructure.mapper.TrainLinesMapper">

    <select id="findById" resultType="landevu.infrastructure.dao.TrainLineDao">
        select * from landevu.train_lines where train_line_id = #{trainLineId}
    </select>

    <select id="findAll" resultType="landevu.infrastructure.dao.TrainLineDao">
        select * from landevu.train_lines
    </select>
</mapper>
