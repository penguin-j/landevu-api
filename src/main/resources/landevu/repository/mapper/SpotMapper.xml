<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="landevu.repository.mapper.SpotMapper">

    <select id="findById" resultType="landevu.repository.dao.SpotDao">
        select * from spots where spot_id = #{spotId}
    </select>

    <select id="findAll" resultType="landevu.repository.dao.SpotDao">
        select * from spots
    </select>
</mapper>
